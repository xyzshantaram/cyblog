{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "store:allow-load",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:allow-watch",
    "fs:allow-unwatch",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive"
  ]
}
